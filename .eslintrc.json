{
    "root": true,
    "env": {
        "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": "off"
    }
}
